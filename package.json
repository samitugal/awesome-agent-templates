{
  "name": "awesome-agent-templates",
  "version": "1.0.0",
  "description": "Framework-agnostic standard and public template library for AI Agents",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run copy-templates",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "generate-schema": "ts-node --esm scripts/generate-schema.ts",
    "preview": "next build && next start",
    "copy-templates": "node -e \"const fs = require('fs'); const path = require('path'); if (!fs.existsSync('public/templates')) fs.mkdirSync('public/templates', {recursive: true}); fs.readdirSync('templates').filter(f => f.endsWith('.yaml')).forEach(f => fs.copyFileSync(path.join('templates', f), path.join('public/templates', f)));\""
  },
  "dependencies": {
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.294.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/js-yaml": "^4.0.9",
    "autoprefixer": "10.4.16",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.3",
    "postcss": "8.4.31",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "ts-node": "^10.9.1"
  },
  "keywords": [
    "ai",
    "agents",
    "templates",
    "framework-agnostic",
    "langchain",
    "semantic-kernel",
    "crewai",
    "mcp"
  ],
  "author": "awesome-agent-templates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samitugal/awesome-agent-templates"
  }
}
